{
  "name": "aidea",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "db:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "dev": "next dev -H 127.0.0.1 -p 5000",
    "build": "next build",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore --ext .ts,.tsx app server components lib",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ext .ts,.tsx app server components lib",
    "postinstall": "prisma generate",
    "preview": "npm run build && next start --port 4173",
    "preview:test": "start-server-and-test preview http://localhost:4173",
    "start": "next start",
    "test": "vitest run --passWithNoTests",
    "test:ci": "vitest run --passWithNoTests",
    "test:coverage": "pnpx vite preview --outDir coverage/lcov-report",
    "test:update": "vitest run -u",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --incremental false"
  },
